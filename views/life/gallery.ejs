<% for(var i=0; i<gallery.length; i++) { %>
    <article style="height: 270px; width: 400px; overflow: hidden;">
        <a href="images/gallery/fulls/<%= gallery[i].post_desp %>" class="image">
            <img src="images/gallery/thumbs/<%= gallery[i].post_desp %>" alt="" />
        </a>
        <div class="caption">
            <h3><%= gallery[i].post_title %></h3>
            <p><%= gallery[i].post %></p>
            <ul class="actions">
                <li><span class="button small">Details</span></li>
            </ul>
        </div>
    </article>
<% } %>